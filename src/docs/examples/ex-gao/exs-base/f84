EXAMPLE  A109_18  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT A_1 B C ;
  MIDPOINT B_1 A C ;
  MIDPOINT C_1 B A ;
  MIDPOINT M1 A_1 B_1 ;
  MIDPOINT M2 A_1 C_1 ;
  INTERSECTION_TT N  M1 M1 A_1  M2 M2 A_1 ;
  INTERSECTION_LT K  A C  A_1 A_1 N ;
  INTERSECTION_LL J  A B  K A_1 ;
SHOW:  COCIRCLE K B C J.	
