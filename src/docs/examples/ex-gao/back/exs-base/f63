EXAMPLE  A_287  
HYPOTHESES:
  POINT A_1 B_1 P ;
  ON_LINE C_1 B_1 A_1;
  MIDPOINT M1 P B_1 ;
  ON_TLINE T1 M1 M1 P;
  MIDPOINT M2 P C_1 ;
  ON_TLINE T2 M2 M2 P;
  MIDPOINT M3 P A_1 ;
  ON_TLINE T3 M3 M3 P;
  INTERSECTION_LL A  M1 T1  M2 T2 ;
  INTERSECTION_LL B  M3 T3  M2 T2 ;
  INTERSECTION_LL C  M1 T1  M3 T3 ;
SHOW:  COCIRCLE A B C P.
