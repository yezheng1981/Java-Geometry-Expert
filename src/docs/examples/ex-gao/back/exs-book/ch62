#Examples in Section 6.1 

EXAMPLE  A_313  
HYPOTHESES:
  POINT A B C ;
  MRATIO D B C r_1;
  MRATIO E C A r_2;
  MRATIO F B A (-1 r_1 r_2);	
  INTERSECTION_LL X D E  A B ;
SHOW: EQ_RATIO A X B X A F B F.

EXAMPLE  E23_21  
HYPOTHESES:
  POINT A B C D X Y ;
  INTERSECTION_LL A_1  A B  X Y ;
  INTERSECTION_LL B_1  B C  X Y ;
  INTERSECTION_LL C_1  C D  X Y ;
  INTERSECTION_LL D_1  A D  X Y ;
SHOW: (RATIO A A_1 B A_1) (RATIO B B_1 C B_1)
      (RATIO C C_1 D C_1) (RATIO D D_1 A D_1) = 1.

EXAMPLE  E23_22  
HYPOTHESES:
  POINT A B C D ;
  MRATIO B1 B C r_1;
  MRATIO C1 C D r_2;
  INTERSECTION_LL D1  D A  B1 C1 ;
  INTERSECTION_LL A1 A B B1 C1;
  MRATIO A2 B A (1 r_1 r_2 (RATIO D D1 D1 A));
SHOW: EQ_RATIO A1 A A1 B A2 A A2 B.

EXAMPLE  A_321  
HYPOTHESES:
  POINT A B C ;
  LRATIO D B C r_1;
  LRATIO E A C r_2;
  INTERSECTION_LL F  A B  D E ;
  PRATIO D_1 B C D -1;
  PRATIO E_1 C A E -1;
  PRATIO F_1 A B F -1;
SHOW: (RATIO A F_1 F_1 B ) (RATIO B D_1 D_1 C ) (RATIO C E_1 E_1 A ) =  -1 .

#ceva
EXAMPLE  A_328  
HYPOTHESES:
  POINT A B C ;
  MRATIO D B C r_1;
  MRATIO E C A r_2;
  MRATIO F B A (r_1 r_2);
  INTERSECTION_LL O  B E  A D ;
  INTERSECTION_LL Z  B A  O C ;
SHOW: (RATIO B Z A Z ) = (RATIO B F A F ).

EXAMPLE  A_340  
HYPOTHESES:
  POINT A B C S ;
  INTERSECTION_LL L  B C  A S ;
  INTERSECTION_LL N  B A  C S ;
  INTERSECTION_LL M  A C  B S ;
SHOW:  (RATIO S L A L ) + (RATIO S M B M ) + (RATIO S N C N ) =  1 .

EXAMPLE  CEVA_1  
HYPOTHESES:
  POINT A B C S ;
  INTERSECTION_LL L  B C  S A ;
  INTERSECTION_LL M  A C  S B ;
  INTERSECTION_LL N  A B  S C ;
SHOW: (AREA A M L) (AREA B N M) (AREA C L N ) = 
      (AREA A N L) (AREA B L M) (AREA C N M ).

EXAMPLE  A164_3  
HYPOTHESES:
  POINT A B C O ;
  ON_LINE A_1 B C;
  ON_LINE B_1 C A;
  ON_LINE C_1 B A;
  INTERSECTION_LP P  B C  A A_1 O ;
  INTERSECTION_LP Q  A C  B B_1 O ;
  INTERSECTION_LP R  A B  C C_1 O ;
SHOW:  (RATIO O A_1 A P ) + (RATIO O B_1 B Q ) + (RATIO O C_1 C R ) =  1 .

EXAMPLE  A_342  
HYPOTHESES:
  POINT A B C S ;
  INTERSECTION_LL L  B C  A S ;
  INTERSECTION_LL N  B A  C S ;
  INTERSECTION_LL M  A C  B S ;
SHOW:  (RATIO A M M C ) + (RATIO A N N B ) = (RATIO A S S L ).

EXAMPLE  CEVA_5  
HYPOTHESES:
  POINT A B C D E O ;
  INTERSECTION_LL A1  O A  C D ;
  INTERSECTION_LL B1  O B  D E ;
  INTERSECTION_LL C1  O C  E A ;
  INTERSECTION_LL D1  O D  A B ;
  INTERSECTION_LL E1  O E  B C ;
SHOW: (RATIO C A1 A1 D )(RATIO D B1 B1 E )(RATIO E C1 C1 A )(RATIO A D1 D1 B )(RATIO B E1 E1 C ) =  1 .

EXAMPLE  A_335  
HYPOTHESES:
  POINT A B C O ;
  INTERSECTION_LL D  A O  C B ;
  INTERSECTION_LL E  B O  A C ;
  INTERSECTION_LL F  C O  A B ;
  PRATIO D_1 B C D -1;
  PRATIO E_1 C A E -1;
  PRATIO F_1 A B F -1;
SHOW: (RATIO A F_1 F_1 B )(RATIO B D_1 D_1 C )(RATIO C E_1 E_1 A ) =  1 .

EXAMPLE  H189_200  
HYPOTHESES:
  POINT O A B C_1 D_1 ;
  INTERSECTION_LL C  A B  O C_1 ;
  INTERSECTION_LL D  A B  O D_1 ;
  INTERSECTION_LL A_1  C_1 D_1  O A ;
  INTERSECTION_LL B_1  C_1 D_1  O B ;
SHOW: (CROSS_RATIO A B C D ) = (CROSS_RATIO A_1 B_1 C_1 D_1 ).

EXAMPLE  CR5  
HYPOTHESES:
  POINT O A A1 ;
  MRATIO B A O r_1;
  MRATIO C O A r_2;
  MRATIO B1 O A1 r_3;
  MRATIO C1 O A1 ((1 r_1 r_2 r_3));
  INTERSECTION_LL X  A A1  B B1 ;
  INTERSECTION_LL Y  A A1  C C1 ;
SHOW: EQ_RATIO A X X A1 A Y Y A1.

EXAMPLE  A_346  
HYPOTHESES:
  POINT A B ;
  MRATIO C A B ((1 r));
  MRATIO D A B ((-1 r));
  MIDPOINT O A B ;
SHOW: EQ_RATIO O C O A O A O D.

EXAMPLE  A_347  
HYPOTHESES:
  POINT A B ;
  MIDPOINT O A B ;
  LRATIO C O A ((1 r));
  LRATIO D O A (1) (r);
SHOW:  HARMONIC A B C D.

EXAMPLE  A_66  
HYPOTHESES:
  POINT A B ;
  MRATIO C A B ((1 r));
  MRATIO D A B ((-1 r));
  MIDPOINT O A B ;
  MIDPOINT M C D ;
SHOW: (RATIO A B M O )(RATIO A B M O ) + (RATIO C D M O )(RATIO C D M O ) =  4 .

EXAMPLE  HARMONIC_SET  
HYPOTHESES:
  POINT A B P Q;
  INTERSECTION_LL R A Q B P;
  INTERSECTION_LL S Q B  A P ;
  INTERSECTION_LL C A B  S R ;
  INTERSECTION_LL F A B  Q P ;
SHOW:  HARMONIC A B C F.

EXAMPLE  A_351  
HYPOTHESES:
  POINT A B O ;
  MRATIO C A B ((1 r));
  MRATIO D A B ((-1 r));
  INTERSECTION_LP P  O C  B O A ;
  INTERSECTION_LP Q  O D  B O A ;
SHOW:  MIDPOINT B P Q.


EXAMPLE  A_352  
HYPOTHESES:
  POINT A B O ;
  ON_LINE C A B;
  INTERSECTION_LP P  O C  B O A ;
  LRATIO Q B P -1;
  INTERSECTION_LL D  O Q  A B ;
SHOW:  HARMONIC A B C D.

#pappus
EXAMPLE  PAPPUS  
HYPOTHESES:
  POINT A A_1 B B_1 Q ;
  INTERSECTION_LL C A B A_1 Q;
  INTERSECTION_LL C_1 A_1 B_1 A Q;  
  INTERSECTION_LL P  A_1 B  A B_1 ;
  INTERSECTION_LL S  B_1 C  B C_1 ;
  INTERSECTION_LL T  B_1 C  P Q ;
SHOW: EQ_RATIO B_1 S C S B_1 T C T.

EXAMPLE  PAPPUS_DUAL  
HYPOTHESES:
  POINT A B C O O_1 ;
  INTERSECTION_LL A_1  O_1 B  O C ;
  INTERSECTION_LL B_1  O A  O_1 C ;
  INTERSECTION_LL C_1  O B  O_1 A ;
  INTERSECTION_LL I  B B_1  A A_1 ;
SHOW:  COLLINEAR C C_1 I.


EXAMPLE  PAPPUS_POINT  
HYPOTHESES:
  POINT A A_1 B B_1 F ;
  INTERSECTION_LL C  A B  A_1 F ;
  INTERSECTION_LL C_1  A_1 B_1  A F ;
  INTERSECTION_LL E  A_1 B  A B_1 ;
  INTERSECTION_LL G  B C_1  A A_1 ;
  INTERSECTION_LL H  B B_1  C A_1 ;
  INTERSECTION_LL I  B B_1  A C_1 ;
  INTERSECTION_LL J  B A_1  C C_1 ;
  INTERSECTION_LL K  G H  E F ;
SHOW:  COLLINEAR I J K.

EXAMPLE  LEIS  
HYPOTHESES:
  POINT A B A_1 B_1 ;
  ON_LINE C A B;
  ON_LINE C_1 A_1 B_1;
  INTERSECTION_LL P  A B_1  B A_1 ;
  INTERSECTION_LL Q  A_1 C  A C_1 ;
  INTERSECTION_LL S  B C_1  B_1 C ;
  INTERSECTION_LL O  A B  A_1 B_1 ;
  INTERSECTION_LL L1  O P  C C_1 ;
  INTERSECTION_LL L2  O Q  B B_1 ;
  INTERSECTION_LL Z1  L1 L2  A A_1 ;
  INTERSECTION_LL Z2  L1 L2  O S ;
SHOW: (RATIO L1 Z1 L2 Z1 ) = (RATIO L1 Z2 L2 Z2 ).

EXAMPLE  A_343  
HYPOTHESES:
  POINT A B C S ;
  ON_LINE A_1 A S;
  ON_LINE B_1 B S;
  ON_LINE C_1 C S;
  INTERSECTION_LL P  B_1 C_1  B C ;
  INTERSECTION_LL Q  A_1 C_1  A C ;
  INTERSECTION_LL R  A_1 B_1  A B ;
SHOW:  COLLINEAR P Q R.

EXAMPLE  A_343_1  
HYPOTHESES:
  POINT A B C A_1 ;
  ON_LINE P B C;
  ON_LINE Q A C;
  INTERSECTION_LL R  A B  P Q ;
  ON_LINE B_1 R A_1;
  INTERSECTION_LL C_1  A_1 Q  B_1 P ;
  INTERSECTION_LL S  A A_1  B B_1 ;
  INTERSECTION_LL Z  A A_1  C C_1 ;
SHOW: EQ_RATIO A S S A_1 A Z Z A_1.

EXAMPLE  E24_16  
HYPOTHESES:
  POINT A A_1 B C C_1 ;
  INTERSECTION_LL O  A C_1  A_1 C ;
  INTERSECTION_LL H  A_1 B  C C_1 ;
  INTERSECTION_LL B_1  A H  B O ;
  INTERSECTION_LL I  B_1 C  A A_1 ;
SHOW:  COLLINEAR C_1 B I.

EXAMPLE  E23_27_Nehring_1942  
HYPOTHESES:
  POINT B C A O ;
  INTERSECTION_LL A_1  A O  B C ;
  INTERSECTION_LL B_1  B O  C A ;
  INTERSECTION_LL C_1  C O  B A ;
  ON_LINE X_1 B C;
  INTERSECTION_LL X_2  B_1 X_1  B A ;
  INTERSECTION_LL X_3  A_1 X_2  C A ;
  INTERSECTION_LL X_4  C_1 X_3  B C ;
  INTERSECTION_LL X_5  B_1 X_4  B A ;
  INTERSECTION_LL X_6  A_1 X_5  C A ;
  INTERSECTION_LL Z2  C A  C_1 X_1 ;
  INTERSECTION_LL Z1  A_1 X_5  C_1 X_1 ;
SHOW: (RATIO C_1 Z1 X_1 Z1 ) (RATIO X_1 Z2 C_1 Z2 ) =  1 .

EXAMPLE  Y_27  
HYPOTHESES:
  POINT A A_1 B B_1 B_2 C ;
  INTERSECTION_LL P  A_1 B_1  A B ;
  ON_LINE Q A C;
  INTERSECTION_LL R  P Q  B C ;
  INTERSECTION_LL C_1  B_1 R  A_1 Q ;
  ON_LINE A_2 B_2 P;
  INTERSECTION_LL C_2  B_2 R  A_2 Q ;
  INTERSECTION_LL I  B B_1  A A_1 ;
  INTERSECTION_LL J  B B_2  A A_2 ;
  INTERSECTION_LL K  B_1 B_2  A_1 A_2 ;
SHOW:  COLLINEAR I J K.


EXAMPLE  A165_9  
HYPOTHESES:
  POINT A B C M ;
  INTERSECTION_LL A_1  B C  A M ;
  INTERSECTION_LL B_1  A C  B M ;
  INTERSECTION_LL C_1  A B  C M ;
  INTERSECTION_PP C_2  B A_1 C_1  A B_1 C_1 ;
  INTERSECTION_LP A_2  C_2 B  C A_1 B_1 ;
  INTERSECTION_LL B_2  C_2 A  A_2 C ;
  INTERSECTION_LL P  B B_2  A A_2 ;
SHOW:  COLLINEAR C C_2 P.

EXAMPLE  C73_1  
HYPOTHESES:
  POINT B C E ;
  ON_PLINE D C B E;
  INTERSECTION_PP A  B D E  D B C ;
  INTERSECTION_PP F  A C D  E B C ;
  MIDPOINT N_1 A C ;
  MIDPOINT N_2 C E ;
  INTERSECTION_LL G  A N_2  E N_1 ;
  INTERSECTION_LL H  D F  G B ;
SHOW:  PARALLEL C F A B.

EXAMPLE  A165_10  
HYPOTHESES:
  POINT A B C O ;
  INTERSECTION_LL D  B C  A O ;
  INTERSECTION_LL E  A C  B O ;
  INTERSECTION_LL F  A B  C O ;
  MIDPOINT D_1 A D ;
  MIDPOINT E_1 B E ;
  MIDPOINT F_1 C F ;
  MIDPOINT A_1 B C ;
  MIDPOINT B_1 C A ;
  MIDPOINT C_1 A B ;
  INTERSECTION_LL I  B_1 E_1  A_1 D_1 ;
SHOW:  COLLINEAR I C_1 F_1.


EXAMPLE  E23_26  
HYPOTHESES:
  POINT A B C O U ;
  INTERSECTION_LL P  B C  A O ;
  INTERSECTION_LL Q  C A  B O ;
  INTERSECTION_LL R  A B  C O ;
  INTERSECTION_LL X  Q R  P U ;
  INTERSECTION_LL Y  R P  Q U ;
  INTERSECTION_LL Z  P Q  R U ;
  INTERSECTION_LL I  B Y  A X ;
SHOW:  COLLINEAR C Z I.

EXAMPLE  Y_38_B  
HYPOTHESES:
  POINT A B C O ;
  INTERSECTION_LL A_1  B C  A O ;
  INTERSECTION_LL B_1  A C  B O ;
  INTERSECTION_LL C_1  A B  C O ;
  ON_LINE A_2 B_1 C_1;
  ON_LINE B_2 A_1 C_1;
  INTERSECTION_LL C_2  A_2 B_2  A_1 B_1 ;
  INTERSECTION_LL A_3  B C  A A_2 ;
  INTERSECTION_LL B_3  A C  B B_2 ;
  INTERSECTION_LL C_3  A B  C C_2 ;
SHOW:  COLLINEAR A_3 B_3 C_3.

EXAMPLE  A158_5  
HYPOTHESES:
  POINT A B C ;
  ON_LINE P B C;
  ON_LINE Q C A;
  INTERSECTION_LL R  P Q  A B ;
  ON_LINE P_1 B C;
  ON_LINE Q_1 C A;
  INTERSECTION_LL R_1  P_1 Q_1  A B ;
  INTERSECTION_LL X  Q R_1  B C ;
  INTERSECTION_LL Y  R P_1  C A ;
  INTERSECTION_LL Z  P Q_1  A B ;
SHOW:  COLLINEAR X Y Z.


EXAMPLE  A165_8  
HYPOTHESES:
  POINT A B C O ;
  INTERSECTION_LL D  B C  A O ;
  INTERSECTION_LL E  A C  B O ;
  INTERSECTION_LL F  A B  C O ;
  MIDPOINT A_1 E F ;
  MIDPOINT B_1 F D ;
  MIDPOINT C_1 D E ;
  INTERSECTION_LL I  B B_1  A A_1 ;
SHOW:  COLLINEAR I C C_1.


EXAMPLE  E23_25  
HYPOTHESES:
  POINT A B C ;
  ON_LINE D B C;
  ON_LINE E A C;
  INTERSECTION_LL F  E D  A B ;
  MIDPOINT P E F ;
  MIDPOINT Q F D ;
  MIDPOINT R D E ;
  INTERSECTION_LL X  A P  B C ;
  INTERSECTION_LL Y  B Q  A C ;
  INTERSECTION_LL Z  C R  A B ;
SHOW:  COLLINEAR X Y Z.


EXAMPLE  A52_1  
HYPOTHESES:
  POINT A B C L ;
  INTERSECTION_LL A_1  B C  A L ;
  INTERSECTION_LL B_1  A C  B L ;
  INTERSECTION_LL C_1  A B  C L ;
  INTERSECTION_LP P  A C  A_1 B B_1 ;
  INTERSECTION_LP Q  A B  A_1 C C_1 ;
  INTERSECTION_LP R  B B_1  A_1 A C ;
SHOW:  COLLINEAR P Q R.


EXAMPLE  AMS_2  
HYPOTHESES:
  POINT A B D E ;
  ON_LINE C A B;
  INTERSECTION_LL F  E D  A B ;
  INTERSECTION_LL I  D B  A E ;
  INTERSECTION_LL J  C D  A E ;
  INTERSECTION_LL L  C D  B E ;
  INTERSECTION_LL G  B E  A D ;
  INTERSECTION_LL K  C E  D B ;
  INTERSECTION_LL H  E C  A D ;
  INTERSECTION_LL O  G J  A B ;
SHOW:  COLLINEAR O H I.