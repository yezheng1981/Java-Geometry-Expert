#6.4.1
EXAMPLE  A_240  
HYPOTHESES:
  POINT A B C D ;
  MIDPOINT E A B ;
  MIDPOINT F B C ;
  MIDPOINT G C D ;
  MIDPOINT H D A ;
SHOW: (RATIO H E G F ) =  1 .

EXAMPLE  A_241  
HYPOTHESES:
  POINT A B C D ;
  MIDPOINT E A B ;
  MIDPOINT F B C ;
  MIDPOINT G C D ;
  MIDPOINT H D A ;
SHOW: 2 (AREA E F G H ) = (AREA A B C D ).

EXAMPLE  A_242  
HYPOTHESES:
  POINT A B C D ;
  MIDPOINT E A B ;
  MIDPOINT G C D ;
  MIDPOINT N A C ;
  MIDPOINT M B D ;
  INTERSECTION_LL O  E G  N M ;
SHOW:  MIDPOINT O N M.

EXAMPLE  A_244  
HYPOTHESES:
  POINT A B C D ;
  CENTROID A_1 B C D ;
  MIDPOINT N A C ;
  MIDPOINT M B D ;
  INTERSECTION_LL J  A A_1  N M ;
SHOW:  MIDPOINT J N M.

EXAMPLE  A_246  
HYPOTHESES:
  POINT A B C D ;
  MIDPOINT E A C ;
  MIDPOINT F B D ;
SHOW: (SQ_DIS A B ) + (SQ_DIS C B ) + (SQ_DIS C D ) + (SQ_DIS D A ) =
      (SQ_DIS A C ) + (SQ_DIS B D ) +  4 (SQ_DIS E F ).

EXAMPLE  A_248  
HYPOTHESES:
  POINT A B C D ;
  MIDPOINT P A B ;
  MIDPOINT Q B C ;
  MIDPOINT S D A ;
  MIDPOINT R C D ;
SHOW:  + (SQ_DIS A C ) + (SQ_DIS B D ) =  +  2 (SQ_DIS Q S ) +  2 (SQ_DIS P R ).

EXAMPLE  C314  
HYPOTHESES:
  POINT A B C D ;
  MIDPOINT X A C ;
  MIDPOINT Y B D ;
  INTERSECTION_LL W  A D  B C ;
SHOW: (AREA A B C D ) = 4 (AREA W X Y ).

EXAMPLE  A127_3  
HYPOTHESES:
  POINT A B C D ;
  MIDPOINT N A C ;
  MIDPOINT M B D ;
  LRATIO P N M r;
SHOW:  + (AREA P A B ) + (AREA P C D ) =  + (AREA P D A ) + (AREA P B C ).

EXAMPLE  M_80  
HYPOTHESES:
  POINT A B C D ;
  MIDPOINT E B C ;
  MIDPOINT F A D ;
  INTERSECTION_LL M  A B  E F ;
  INTERSECTION_LL N  C D  E F ;
SHOW: (RATIO A M B M ) = (RATIO D N C N ).

#undo
EXAMPLE  M_8  
HYPOTHESES:
  POINT A B C X ;
  INTERSECTION_LC D  C X  C (SQ_DIS A B);
  MIDPOINT F A D ;
  MIDPOINT E B C ;
  INTERSECTION_LL M  A B  E F ;
  INTERSECTION_LL N  C D  E F ;
SHOW:  EQANGLE B M E E N C.

#undo
EXAMPLE  M_81  
HYPOTHESES:
  POINT A B C X ;
  INTERSECTION_LC D  C X  C (SQ_DIS A B);
  MIDPOINT F A D ;
  MIDPOINT E B C ;
  INTERSECTION_LL M  A B  E F ;
  INTERSECTION_LL N  C D  E F ;
SHOW:  EQDISTANCE A M D N.

#undo
EXAMPLE  D190  
HYPOTHESES:
  POINT A B C X ;
  INTERSECTION_LC D  C X  C (SQ_DIS A B)  ;
  MIDPOINT P A D ;
  MIDPOINT Q B C ;
  MIDPOINT N A C ;
  MIDPOINT M B D ;
SHOW:  PERPENDICULAR N M P Q.

EXAMPLE  A31_35  
HYPOTHESES:
  POINT A B C D ;
  INTERSECTION_LL O  A B  C D ;
  INTERSECTION_LL O1  A D  B C ;
  LRATIO E O B ((-1 (RATIO A B B O)));
  LRATIO F O C ((-1 (RATIO D C C O)));
  LRATIO E_1 O1 A ((-1 (RATIO A D A O1)));
  LRATIO F_1 O1 B ((-1 (RATIO B C B O1)));
SHOW: (AREA E F E_1 ) = (AREA E F F_1 ).

EXAMPLE  A127_2  
HYPOTHESES:
  POINT A B C D O ;
  MIDPOINT P A B ;
  MIDPOINT Q B C ;
  MIDPOINT R C D ;
  MIDPOINT S D A ;
  MIDPOINT U A C ;
  MIDPOINT V D B ;
  LRATIO P_1 O P Y_T;
  LRATIO Q_1 O Q Y_T;
  LRATIO R_1 O R Y_T;
  LRATIO S_1 O S Y_T;
  LRATIO U_1 O U Y_T;
  LRATIO V_1 O V Y_T;
  INTERSECTION_LL J  P_1 R_1  Q_1 S_1 ;
  INTERSECTION_LL Z_1  P_1 R_1  U_1 V_1 ;
  INTERSECTION_LL Z_2  U_1 V_1  Q_1 S_1 ;
SHOW: 1 (RATIO U_1 Z_1 V_1 Z_1 ) = 1 (RATIO U_1 Z_2 V_1 Z_2 ).

EXAMPLE  PRATT_4  
HYPOTHESES:
  POINT A B C D ;
  MIDPOINT M A B ;
  ON_TLINE M1 M M B;
  MIDPOINT N B C ;
  ON_TLINE N1 N N B;
  MIDPOINT P C D ;
  ON_TLINE P1 P P D;
  MIDPOINT Q D A ;
  ON_TLINE Q1 Q Q D;
  INTERSECTION_LL D_1  M M1  N N1 ;
  INTERSECTION_LL A_1  N N1  P P1 ;
  INTERSECTION_LL C_1  M M1  Q Q1 ;
  INTERSECTION_LL B_1  P P1  Q Q1 ;
  ON_TLINE X B B C_1;
  INTERSECTION_LT E  B X  A A D_1 ;
  INTERSECTION_LT H  A E  C C B_1 ;
  INTERSECTION_LT F  E B  D D A_1 ;
  INTERSECTION_LL G  C H  F D ;
SHOW: (RATIO H A A E )(RATIO E B B F )(RATIO F D D G )(RATIO G C C H ) =  1 .

EXAMPLE  TRAP3  
HYPOTHESES:
  POINT A B C ;
  ON_PLINE D C A B;
  MIDPOINT Q A C ;
  MIDPOINT P B D ;
SHOW:  PARALLEL P Q A B.

EXAMPLE  D252  
HYPOTHESES:
  POINT A B C ;
  ON_PLINE D A B C;
  MIDPOINT P B D ;
  MIDPOINT Q A C ;
SHOW:  + (RATIO B C P Q ) - (RATIO A D P Q ) =  2 .

EXAMPLE  TRAP  
HYPOTHESES:
  POINT A B C ;
  ON_PLINE D C A B;
  INTERSECTION_LL O  A C  B D ;
  INTERSECTION_LP E  B C  O A B ;
  INTERSECTION_LP F  A D  O A B ;
SHOW:  MIDPOINT O E F.

EXAMPLE  HARMONIC_SET  
HYPOTHESES:
  POINT A B P Q ;
  INTERSECTION_LL R  A Q  B P ;
  INTERSECTION_LL S  Q B  A P ;
  INTERSECTION_LL C  A B  S R ;
  INTERSECTION_LL F  A B  Q P ;
SHOW:  HARMONIC A B C F.

EXAMPLE  F141  
HYPOTHESES:
  POINT A B C D E ;
  INTERSECTION_LL P  A B  C D ;
  INTERSECTION_LL Q  B C  A D ;
  INTERSECTION_LL S  A C  P Q ;
  ON_LINE G E S;
  INTERSECTION_LL F  P E  Q G ;
  INTERSECTION_LL H  E Q  P G ;
  INTERSECTION_LL R  B D  P Q ;
  INTERSECTION_LL O  F H  P Q ;
SHOW: EQ_RATIO P R Q R P O Q O.

EXAMPLE  F140  
HYPOTHESES:
  POINT A B C D P ;
  LRATIO U A D r_1;
  LRATIO V B C r_2;
  INTERSECTION_LL I  D B  U V ;
  INTERSECTION_LL J  C D  U V ;
  INTERSECTION_LL K  A B  U V ;
  LRATIO S U P r_3;
  INTERSECTION_LL Q  I S  K P ;
  INTERSECTION_LL R  V Q  S J ;
  INTERSECTION_LL L  P R  U V ;
  INTERSECTION_LL N  A C  U V ;
SHOW: EQ_RATIO U L V L U N V N.

EXAMPLE  GAUSS_POINT  
HYPOTHESES:
  POINT A_0 A_1 A_2 A_3 A_4 ;
  INTERSECTION_LL X  A_3 A_4  A_1 A_0 ;
  INTERSECTION_LL Y  A_2 A_3  A_0 A_4 ;
  INTERSECTION_LL Z  A_4 A_3  A_1 A_2 ;
  MIDPOINT M_1 A_1 A_3 ;
  MIDPOINT M_2 X A_2 ;
  MIDPOINT M_3 A_0 A_2 ;
  MIDPOINT M_4 Y A_1 ;
  MIDPOINT M_5 A_1 A_4 ;
  MIDPOINT M_6 A_0 Z ;
  INTERSECTION_LL I  M_3 M_4  M_1 M_2 ;
SHOW:  COLLINEAR I M_5 M_6.



EXAMPLE  A70_9  
HYPOTHESES:
  POINT A B C D ;
  CENTROID A_1 B C D ;
  CENTROID D_1 A B C ;
  INTERSECTION_LL J  A A_1  D D_1 ;
SHOW: 1 (RATIO A J A_1 J ) =  -3 .

EXAMPLE  A_305  
HYPOTHESES:
  POINT A B C ;
  ON_LINE D A B;
  ON_LINE E A C;
  INTERSECTION_LL F  E D  C B ;
  FOOT G_1 C A B ;
  INTERSECTION_LT H_1  C G_1  A B C ;
  FOOT G_2 E A B ;
  INTERSECTION_LT H_2  E G_2  A E D ;
  FOOT G_3 F A B ;
  INTERSECTION_LT H_3  F G_3  B E D ;
  INTERSECTION_LL H_4  F G_3  H_1 H_2 ;
SHOW: EQ_RATIO F H_3 G_3 H_3 F H_4 G_3 H_4.

