EXAMPLE  A_259  
HYPOTHESES:
  CIRCLE A B C D ;
  CIRCUMCENTER O A B C ;
  MIDPOINT T A D ;
  MIDPOINT P A B ;
  MIDPOINT Q B C ;
  MIDPOINT R C D ;
  INTERSECTION_PP M  P O R  Q O T ;
  INTERSECTION_LL I  P R  O M ;
SHOW:  MIDPOINT I P R.

EXAMPLE  A134_5  
HYPOTHESES:
  CIRCLE A B C D ;
  CIRCUMCENTER O A B C ;
  INTERSECTION_LL I  A D  B C ;
  MIDPOINT Q B C ;
  MIDPOINT S A D ;
  MIDPOINT J S Q ;
  LRATIO M J O -1;
SHOW:  PERPENDICULAR M S B C.

EXAMPLE  A134_6  
HYPOTHESES:
  CIRCLE A B C D ;
  CIRCUMCENTER O A B C ;
  MIDPOINT Q B C ;
  MIDPOINT S A D ;
  MIDPOINT J S Q ;
  LRATIO Q_1 Q O -1;
  LRATIO S_1 S O -1;
  INTERSECTION_LL M  O J  Q_1 S_1 ;
SHOW:  MIDPOINT J O M.

EXAMPLE  A135_8  
HYPOTHESES:
  CIRCLE A B C D E ;
  FOOT P E A B ;
  FOOT Q E B C ;
  FOOT R E C D ;
  FOOT S E A D ;
SHOW:  EQ_PRODUCT E S E Q E P E R.

EXAMPLE  A158_6  
HYPOTHESES:
  CIRCLE A B C D P ;
  CIRCUMCENTER O A B C ;
  FOOT A_1 P A B ;
  FOOT B_1 P B C ;
  FOOT C_1 P C D ;
  FOOT D_1 P D A ;
SHOW: (RATIO A A_1 A_1 B )(RATIO B B_1 B_1 C )(RATIO C C_1 C_1 D )(RATIO D D_1 D_1 A ) =  1 .

EXAMPLE  A_261  
