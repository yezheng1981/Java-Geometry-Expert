EXAMPLE  NEWTON  
HYPOTHESES:
  CIRCLE A B C D ;
  CIRCUMCENTER O A B C ;
  ON_TLINE B_T B B O;
  ON_TLINE A_T A A O;
  ON_TLINE C_T C C O;
  ON_TLINE D_T D D O;
  INTERSECTION_LL A_1  B B_T  A A_T ;
  INTERSECTION_LL B_1  C C_T  B B_T ;
  INTERSECTION_LL C_1  D D_T  C C_T ;
  INTERSECTION_LL D_1  A A_T  D D_T ;
  MIDPOINT M A_1 C_1 ;
  INTERSECTION_LL N  B_1 D_1  O M ;
SHOW:  MIDPOINT N B_1 D_1.

#6.4.7
EXAMPLE  C281  
HYPOTHESES:
  CIRCLE A B C D ;
  CIRCUMCENTER O A B C ;
  INTERSECTION_LL E  A B  C D ;
  INTERSECTION_LT G  B C  E E O ;
  INTERSECTION_LT F  A D  E E O ;
SHOW: (RATIO G F E F ) =  2 .

EXAMPLE  BTF1  
HYPOTHESES:
  POINT A C ;
  ON_BLINE B A C;
  MIDPOINT M A C ;
  ON_LINE D B M;
  ON_LINE P A B;
  INTERSECTION_LL Q  C D  P M ;
  ON_LINE S B C;
  INTERSECTION_LL R  A D  S M ;
  INTERSECTION_LL G  A C  P R ;
  INTERSECTION_LL H  A C  S Q ;
SHOW: EQ_RATIO M G G A M H H C.

EXAMPLE  BTF2  
HYPOTHESES:
  POINT A C B ;
  MIDPOINT M A C ;
  LRATIO D M B r_1;
  LRATIO P A B r_2;
  LRATIO R A D r_3;
  INTERSECTION_LL Q  P M  C D ;
  INTERSECTION_LL S  R M  B C ;
  INTERSECTION_LL G  A C  P R ;
  INTERSECTION_LL H  A C  S Q ;
SHOW: EQ_RATIO M G G A M H H C.

EXAMPLE  BTF3  
HYPOTHESES:
  POINT A B C ;
  LRATIO M A C r_0;
  LRATIO D M B r_1;
  LRATIO P A B r_2;
  LRATIO R A D r_3;
  INTERSECTION_LL Q  P M  C D ;
  INTERSECTION_LL S  R M  B C ;
  INTERSECTION_LL G  A C  P R ;
  INTERSECTION_LL H  A C  S Q ;
SHOW: (RATIO M G A G ) = (RATIO M H C H )(RATIO C M M A).