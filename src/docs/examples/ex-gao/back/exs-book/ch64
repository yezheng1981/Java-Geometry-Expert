EXAMPLE  D204  
HYPOTHESES:
  POINT A B C ;
#  INTERSECTION_PP D A B C C A B;
  PRATIO D A B C 1;
  FOOT F A B D ;
  FOOT E B A C ;
  FOOT H C B D ;
  FOOT G D A C ;
SHOW: (RATIO E H F G ) =  1 .

EXAMPLE  D208  
HYPOTHESES:
  POINT A B C ;
#  INTERSECTION_PP D A B C C A B;
  PRATIO D A B C 1;
  MIDPOINT L A D ;
  CONSTANT i^2+1 ;
  PSQUARE S L D ;
  MIDPOINT N B C ;
  PSQUARE Q N B ;
  MIDPOINT T A B ;
  PSQUARE P T A ;
SHOW:  (VEC P S ) -  i (VEC P Q ) =  0 .

EXAMPLE  D208_2  
HYPOTHESES:
  POINT A B C ;
#  INTERSECTION_PP D A B C C A B;
  PRATIO D A B C 1;
  MIDPOINT L A D ;
  CONSTANT (i i) -1 ;
  PSQUARE S L D ;
  MIDPOINT N B C ;
  PSQUARE  Q N B ;
  MIDPOINT T A B ;
  NSQUARE P T A ;
  MIDPOINT M C D ;
  NSQUARE  R M C ;
SHOW: (VEC P S ) = (VEC Q R ).

EXAMPLE  D208_1  
HYPOTHESES:
  POINT A B C ;
#  INTERSECTION_PP D A B C C A B;
  PRATIO D A B C 1;
  MIDPOINT L A D ;
  CONSTANT (i i) -1 ;
  PSQUARE S L D ;
  MIDPOINT N B C ;
  PSQUARE Q N B ;
  MIDPOINT O Q S ;
SHOW:  + (VEC A O ) - (VEC O C ) =  0 .

EXAMPLE  D208_3  
HYPOTHESES:
  POINT A B C ;
  INTERSECTION_PP D A B C C A B;
#  PRATIO D A B C 1;
  TRATIO A1 A A D r_1;
  INTERSECTION_PP D1 A1 A D D A1 A;
  INTERSECTION_LL S  A D1  D A1 ;
  MIDPOINT N B C ;
  PRATIO Q N A1 A (1) (2);
  TRATIO A2 A A B r_2;
  INTERSECTION_PP B1 A2 A B B A2 A;
  INTERSECTION_LL P  A B1  B A2 ;
  MIDPOINT M C D ;
  PRATIO R M A2 A (1) (2);
SHOW: (RATIO P S Q R ) =  1 .

EXAMPLE  Y_15_A  
HYPOTHESES:
  POINT A B C ;
#  INTERSECTION_PP D  A B C  C A B ;
  PRATIO D A B C 1;
  LRATIO A_1 C D (1) (3);
  LRATIO B_1 D A (1) (3);
  INTERSECTION_LL A_2  A A_1  B B_1 ;
SHOW: 3 (AREA A B C D ) = 13 (AREA A B A_2 ).

EXAMPLE  PGRAM2  
HYPOTHESES:
  POINT A B C ;
#  INTERSECTION_PP D  A B C  C A B ;
  PRATIO D A B C 1;
  LRATIO A_1 C D r_1;
  LRATIO B_1 D A r_2;
  LRATIO C_1 A B r_1;
  LRATIO D_1 B C r_2;
  INTERSECTION_LL A_2  B B_1  A A_1 ;
  INTERSECTION_LL B_2  C C_1  B B_1 ;
  INTERSECTION_LL C_2  D D_1  C C_1 ;
  INTERSECTION_LL D_2  A A_1  D D_1 ;
SHOW: 2 (r_1 r_2-r_2-r_1+2)(AREA A_2 A B ) = (1-r_2)(AREA A B C D ).

EXAMPLE  M10_32  
