  LRATIO F O A (1) (r_2);
  MIDPOINT I E F ;
  INTERSECTION_LC R  P X  U P  ;
  LRATIO G O R ((PY O A O)) ((PY O R O));
SHOW:  EQDISTANCE G I I E.

