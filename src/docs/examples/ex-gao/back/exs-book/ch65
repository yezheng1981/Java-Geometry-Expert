EXAMPLE  BRIANCHON  
HYPOTHESES:
  CIRCLE A B C D E F ;
  CIRCUMCENTER O A B C ;
  ON_TLINE B_T B B O;
  ON_TLINE A_T A A O;
  ON_TLINE C_T C C O;
  ON_TLINE D_T D D O;
  ON_TLINE E_T E E O;
  ON_TLINE F_T F F O;
  INTERSECTION_LL A_1  B B_T  A A_T ;
  INTERSECTION_LL D_1  E E_T  D D_T ;
  INTERSECTION_LL B_1  C C_T  B B_T ;
  INTERSECTION_LL E_1  F F_T  E E_T ;
  INTERSECTION_LL C_1  D D_T  C C_T ;
  INTERSECTION_LL F_1  A A_T  F F_T ;
  INTERSECTION_LL I  B_1 E_1  A_1 D_1 ;
  INTERSECTION_LL J  A_1 D_1  C_1 F_1 ;
SHOW: EQ_RATIO A_1 I D_1 I A_1 J D_1 J.

