
#6.5.1
EXAMPLE  A_410  
HYPOTHESES:
  CIRCLE A B C D ;
  INTERSECTION_LL E  B D  A C ;
SHOW:  EQ_PRODUCT A E C E B E D E.

EXAMPLE  A_412  
HYPOTHESES:
  POINT A B ;
  ON_BLINE O A B;
  ON_LINE E A B;
SHOW: (PY A E B ) =   (SQ_DIS O E ) - (SQ_DIS O A ).

EXAMPLE  TANGENT  
HYPOTHESES:
  POINT A T ;
  ON_BLINE O A T;
  TRATIO P T T O r;
  INTERSECTION_LC B  P A  O A  ;
SHOW:  EQ_PRODUCT P T P T P A P B.

EXAMPLE  M_18  
HYPOTHESES:
  POINT A B D ;
  CIRCUMCENTER O A B D ;
  LRATIO C O D -1;
  FOOT E C A B ;
  FOOT F D A B ;
  MIDPOINT M A B ;
SHOW:  MIDPOINT M E F.

EXAMPLE  CI1  
HYPOTHESES:
  CIRCLE A B C D E P ;
  FOOT L P A B ;
  FOOT M P B C ;
  FOOT N P C D ;
  FOOT K P D E ;
  FOOT S P E A ;
SHOW: (RATIO A L L B )(RATIO B M M C )(RATIO C N N D )(RATIO D K K E )(RATIO E S S A ) =  -1 .

EXAMPLE  A193_9  
HYPOTHESES:
  POINT A B P ;
  ON_TLINE D A A B;
  INTERSECTION_PP C  B A D  D A B ;
  INTERSECTION_LL O  B D  A C ;
  FOOT X P A B ;
  INTERSECTION_LL X_1  C D  P X ;
  FOOT Y P A D ;
  INTERSECTION_LL Y_1  B C  Y P ;
SHOW:  (SQ_DIS O P ) - (SQ_DIS O A ) =   (PY X P X_1 ) +  (PY Y P Y_1 ).

EXAMPLE  M_16  
HYPOTHESES:
  POINT C E ;
  ON_LINE X C E;
  ON_TLINE D E E C;
  MIDPOINT O D C ;
  INTERSECTION_TT A  E E O  C C O ;
  INTERSECTION_LL B  D C  A E ;
  INTERSECTION_LL F  D E  A C ;
SHOW:  EQDISTANCE A F A E.

EXAMPLE  A194_6  
HYPOTHESES:
  POINT A B C ;
  CIRCUMCENTER O A B C ;
  FOOT D A B C ;
  FOOT F C A B ;
  FOOT E B A C ;
  INTERSECTION_LL H  B E  A D ;
  LRATIO A_1 A H -1;
  LRATIO B_1 B H -1;
  LRATIO C_1 C H -1;
SHOW: (SQ_DIS A_1 O ) - (SQ_DIS O B ) + (SQ_DIS B_1 O ) - (SQ_DIS O B ) + (SQ_DIS C_1 O ) - (SQ_DIS O B ) =  + (SQ_DIS A B ) + (SQ_DIS B C ) + (SQ_DIS C A ).


EXAMPLE  A194_7  
HYPOTHESES:
  POINT A B C ;
  ON_TLINE Y B B C;
  INTERSECTION_LB O_1  B Y  A B ;
  INTERSECTION_LB O_2  A O_1  A C ;
  INTERSECTION_LC D  B C  O_2 C  ;
SHOW:  EQANGLE C A B B A D.

EXAMPLE  A227_1  
HYPOTHESES:
  POINT A B C ;
  CENTROID G A B C ;
  CIRCUMCENTER O_1 B C G ;
  CIRCUMCENTER O_2 A C G ;
SHOW:  + (SQ_DIS A O_1 ) - (SQ_DIS O_1 C ) =  + (SQ_DIS B O_2 ) -(SQ_DIS O_2 C ).

EXAMPLE  H_104  
HYPOTHESES:
  POINT A B ;
  ON_BLINE O A B;
  LRATIO C A B r;
  TRATIO X B B O s;
  INTERSECTION_TL E  C C O  B X ;
  INTERSECTION_TL D  A A O  C E ;
SHOW:  MIDPOINT C D E.

EXAMPLE  M22_77  
HYPOTHESES:
  POINT G B ;
  MIDPOINT M B G ;
  ON_TLINE A M M B;
  INTERSECTION_LB O  A M  A B ;
  LRATIO C O B -1;
  FOOT D A B C ;
  INTERSECTION_LL E  A D  B G ;
  INTERSECTION_LL F  A C  B G ;
SHOW:  EQDISTANCE A E B E.

EXAMPLE  M38_156  
HYPOTHESES:
  CIRCLE A B C ;
  CIRCUMCENTER O A B C ;
  LRATIO D B C  (L A B) ( (L A B) + (L A C));
  INTERSECTION_LC E  A D  O A  ;
SHOW: (PY D A A E ) = 2 (L A B )(L A C ).

EXAMPLE  M39_158  
HYPOTHESES:
  POINT E D ;
  ON_BLINE A E D;
#  MIDPOINT M E D;
#  TRATIO A M E D s;
  LRATIO B A E r;
  TRATIO Y D D A 1;
  INTERSECTION_LB O  D Y  B D ;
  LRATIO C O B -1;
  INTERSECTION_LT F  A C  E E A ;
SHOW:  EQ_PRODUCT A B A C A E A F.


EXAMPLE  H199_212A  
HYPOTHESES:
  CIRCLE A B C D E E_1 ;
  INTERSECTION_LL G  A B  E C ;
  INTERSECTION_LL F  A B  E D ;
  INTERSECTION_LL G_1  A B  E_1 C ;
  INTERSECTION_LL F_1  A B  E_1 D ;
SHOW: (CROSS_RATIO A F G B ) = (CROSS_RATIO A F_1 G_1 B ).

EXAMPLE  A171_12  
HYPOTHESES:
  POINT A C ;
  ON_TLINE B C C A;
  MIDPOINT O A B ;
  FOOT E C A O ;
  LRATIO D E C -1;
  INTERSECTION_LT T  A O  C O C ;
  INTERSECTION_LL A_1  T D  C A ;
  INTERSECTION_LL B_1  T D  C B ;
SHOW:  HARMONIC T D A_1 B_1.

