EXAMPLE  PAS_4  
HYPOTHESES:
  POINT A B D E S ;
  ON_LINE C S D;
  INTERSECTION_LL P  D E  A B ;
  INTERSECTION_LL Q  P S  B C ;
  INTERSECTION_LL F  S A  Q E ;
  INTERSECTION_LL S_1  B A  F D ;
  INTERSECTION_LL P_1  D E  A C ;
  INTERSECTION_LL Q_1  E B  C F ;
  INTERSECTION_LL Q_2  B E  P_1 S_1 ;
SHOW: EQ_RATIO E Q_1 B Q_1 E Q_2 B Q_2.

