
#ch3
EXAMPLE  A_635  
HYPOTHESES:
  POINT A B C M ;
  ON_ALINE A1 A C B A M;
  ON_ALINE B1 B A C B M;
  INTERSECTION_LL N  A A1  B B1 ;
SHOW:  EQANGLE A C M N C B.


##cf
EXAMPLE  A_64_10_A
HYPOTHESES:
 POINT U V;
 MIDPOINT M U V;
 INTERSECTION_BC A U V M U;
 ON_LINE B U V;
 ON_ALINE X A U U A B;
 INTERSECTION_LL C U V A X;
 MIDPOINT O B C;
 INTERSECTION_LC P A B O B;
 INTERSECTION_LC Q A C O B;
SHOW: PERP_BISECT C P Q.


EXAMPLE  A_559
HYPOTHESES:
  POINT A B C;
  CIRCUMCENTER O A B C ;
  MIDPOINT A1 B C;
  ON_ALINE X A C B A A1;
  INTERSECTION_LL S A X B C;
  INTERSECTION_LC P A A1 O A;
  INTERSECTION_LC Q A X O A;
SHOW: PARALLEL P Q B C.


EXAMPLE  A_568_A
HYPOTHESES:
  POINT A B C;
  CIRCUMCENTER O A B C ;
  MIDPOINT A1 B C;
  ON_ALINE X A C B A A1;
  INTERSECTION_LL S A X B C;
  ON_LINE N A A1;
  FOOT G N A B;
  FOOT H N A C;
SHOW: PERPENDICULAR G H A X.

EXAMPLE  A_568
HYPOTHESES:
  POINT A B C;
  CIRCUMCENTER O A B C ;
  MIDPOINT A1 B C;
  ON_ALINE X A C B A A1;
  INTERSECTION_LL S A X B C;
  ON_LINE N A X;
  FOOT G N A B;
  FOOT H N A C;
SHOW: PERPENDICULAR G H A A1.

EXAMPLE  A252_11_A
HYPOTHESES:
  POINT A B C;
  CIRCUMCENTER O A B C ;
  MIDPOINT A1 B C;
  ON_ALINE X A C B A A1;
  INTERSECTION_LL S A X B C;
  INTERSECTION_LC P A A1 O A;
  INTERSECTION_LC Q A X O A;
  FOOT D Q B C;
  FOOT G Q A B;
SHOW: PERPENDICULAR D G A P.

EXAMPLE  A252_11
HYPOTHESES:
  POINT A B C;
  CIRCUMCENTER O A B C ;
  MIDPOINT A1 B C;
  ON_ALINE X A C B A A1;
  INTERSECTION_LL S A X B C;
  INTERSECTION_LC P A A1 O A;
  INTERSECTION_LC Q A X O A;
  FOOT E P A C;
  FOOT F P A B;
SHOW: PERPENDICULAR E F A Q.

EXAMPLE  A_626
HYPOTHESES:
  CIRCLE A B C P;
  CIRCUMCENTER O A B C ;
  ON_ALINE X A B C A P;
  INTERSECTION_LC Q A X O A;
SHOW: PARALLEL P Q B C.

EXAMPLE  A_627
HYPOTHESES:
  POINT A B C M;
  ON_ALINE N A C B A M;
  FOOT P M A C;
  FOOT Q M A B;
SHOW: PERPENDICULAR P Q A N.

#unsolved
EXAMPLE  A4165
HYPOTHESES:
 CIRCLE A B C D C1;
 CIRCUMCENTER O A B C;
 ON_ALINE X A C1 D A C;
 INTERSECTION_LC D1 A X O A;
 FOOT P C A B;
 FOOT Q C A D;
 FOOT P1 C1 A B;
 FOOT Q1 C1 A D1;
SHOW: PARALLEL P Q P1 Q1.













