#chpater 5

#undo solid
EXAMPLE  L12_6  
HYPOTHESES:
  POINT A B C D ;
  MIDPOINT S B C ;
  LRATIO Z A S (2) (3);
  LRATIO Y D S (2) (3);
  INTERSECTION_LL G  D Z  A Y ;
SHOW: (VEC G ) =  1 .

EXAMPLE  V_CENT  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT F A C ;
  MIDPOINT E B C ;
  INTERSECTION_LL G  A E  B F ;
SHOW: 3 (VEC A G ) = 2 (VEC A E ).
#der
EXAMPLE  V_CENT1  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT F A C ;
  MIDPOINT E B C ;
  INTERSECTION_LL G  A E  B F ;
SHOW: (VEC G ) =  1 .

EXAMPLE  A_97  
HYPOTHESES:
  POINT A B C X Y ;
  MIDPOINT D B C ;
  MIDPOINT E A C ;
  MIDPOINT F A B ;
  CENTROID G A B C ;
  CENTROID K D E F ;
SHOW: (VEC G ) = (VEC K ).

EXAMPLE  A_100  
HYPOTHESES:
  POINT A B C ;
  INTERSECTION_PP P  A B C  B A C ;
  INTERSECTION_PP Q  A B C  C A B ;
  INTERSECTION_PP R  B A C  C A B ;
  CENTROID G A B C ;
  CENTROID K P Q R ;
SHOW: (VEC G ) = (VEC K ).

EXAMPLE  ORTH_C1  
HYPOTHESES:
  POINT A B C ;
  FOOT E B A C ;
  FOOT D C A B ;
  INTERSECTION_LL H  C D  B E ;
SHOW:  PERPENDICULAR B C A H.

EXAMPLE  A_282_1  
HYPOTHESES:
  CIRCLE A B C D ;
  FOOT E D B C ;
  FOOT F D A C ;
  FOOT G D A B ;
SHOW: (RATIO A G G B )(RATIO B E E C )(RATIO C F F A ) =  -1 .

EXAMPLE  A_258  
HYPOTHESES:
  CIRCLE A B C D ;
  CIRCUMCENTER O A B C ;
  MIDPOINT G A D ;
  MIDPOINT F A B ;
  MIDPOINT E C D ;
  INTERSECTION_PP N  E O F  F O E ;
SHOW:  PERPENDICULAR G N B C.

#wuundo
EXAMPLE  COM1  
HYPOTHESES:
  POINT A B C P ;
  SIM_TRIANGLE Q B C P C A ;
# INTERSECTION_PP R Q C P P Q C;
  PRATIO R Q C P 1;
SHOW: (VEC P A) (VEC A B) = (VEC R A) (VEC A C).

EXAMPLE  COM2  
HYPOTHESES:
  POINT A B P Q ;
  CONSTANT w^2 -3;  
  PE_TRIANGLE C B A ;
  PE_TRIANGLE R Q P ;
  MIDPOINT L A P ;
  MIDPOINT M B Q ;
  MIDPOINT N C R ;
SHOW:  PE_TRIANGLE N M L.

#wuundo
EXAMPLE  COM3  
HYPOTHESES:
  POINT A B P Q X Y ;
  CONSTANT w^2-3;
  PE_TRIANGLE C B A ;
  PE_TRIANGLE R Q P ;
  PE_TRIANGLE Z Y X ;
  CENTROID L A P X ;
  CENTROID M B Q Y ;
  CENTROID N C R Z ;
SHOW:  PE_TRIANGLE N M L.

EXAMPLE  COM5  
HYPOTHESES:
  POINT A B C ;
#area  CONSTANT w^2+w+1;
#wu    CONSTANT w^2-3;
  PE_TRIANGLE B_1 A C ;
  PE_TRIANGLE A_1 C B ;
  NE_TRIANGLE C_1 B A ;
SHOW:  (VEC A_1 C_1 ) - (VEC C B_1 ) =  0 .

EXAMPLE  COM4  
HYPOTHESES:
  POINT A B C ;
#area  CONSTANT w^2 + w + 1;
#wu  CONSTANT w^2-3;
  INTERSECTION_PP D A B C C A B;
  PE_TRIANGLE X B C ;
  PE_TRIANGLE Y C D ;
SHOW:  PE_TRIANGLE A Y X.

EXAMPLE  C237  
HYPOTHESES:
  POINT A B C ;
  CONSTANT i^2+1;
  PSQUARE F A B ;
  NSQUARE H A C ;
SHOW: CONGRUENT F C B H.
#SHOW:  (VEC F C ) -  i (VEC B H ) =  0 .

EXAMPLE  SQ1  
HYPOTHESES:
  POINT A B C ;
  PSQUARE F A B ;
  NSQUARE H A C ;
  MIDPOINT M B C ;
SHOW: PERPENDICULAR H F A M.
#SHOW:  (VEC H F ) -2  i (VEC A M ) =  0 .

EXAMPLE  E_2124_3  
HYPOTHESES:
  POINT A B C ;
  CONSTANT i^2+1;
  NSQUARE F C A ;
  PSQUARE D C B ;
  PRATIO E D  C B 1;
  PSQUARE K B A ;
  PRATIO Q F C D 1;
  PRATIO P K B E 1;
#wu   SHOW: PERPENDICULAR A Q A P.
#wu   SHOW: CONGRUENT A Q A P.
#area SHOW: (VEC A Q ) - i (VEC A P ) =0.
