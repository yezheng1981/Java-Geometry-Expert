EXAMPLE  3_106
HYPOTHESES:
 POINT A B C;
 CIRCUMCENTER O A B C;
 FOOT D A B C;
SHOW: (ANGLE A O A D) - (ANGLE A B B C) + (ANGLE B C C A) = 0.

EXAMPLE  3_109
HYPOTHESES:
 POINT A M N;
 CIRCUMCENTER O A M N;
 FOOT P A O N;
 FOOT Q A O M;
 INTERSECTION_LC C A P O A;
 INTERSECTION_LC B A Q O A;
 INTERSECTION_LL E M N A P;
 INTERSECTION_LL D M N A Q;
SHOW: PERP_BISECT A D E.

EXAMPLE  3_310
HYPOTHESES:
 POINT A B;
 ON_BLINE C A B;
 CIRCUMCENTER O A B C;
 ON_LINE F A B;
 ON_LINE G A B;
 INTERSECTION_LC D C F O C;
 INTERSECTION_LC E C G O C;
SHOW: COCIRCLE G D F E.

EXAMPLE  3_111
HYPOTHESES:
 POINT X;
 CIRCLE R P Q S;
 INTERSECTION_LS Y Q S Q P X;
 INTERSECTION_LL I X Y S R;
SHOW: COCIRCLE X R I P.

EXAMPLE  3_113
HYPOTHESES:
 POINT A B C;
 FOOT F C A B;
 MIDPOINT M B C;
 MIDPOINT Q A C;
 MIDPOINT P A B;
 MIDPOINT L P F;
 CIRCUMCENTER N P Q F;
SHOW: (ANGLE B C A B) + (ANGLE A C A B)
     + (ANGLE F N L N) =0.

EXAMPLE  A111_5  
HYPOTHESES:
  POINT A B C ;
  ORTHOCENTER H A B C ;
  CIRCUMCENTER O_A B C H ;
  CIRCUMCENTER O_B A C H ;
  CIRCUMCENTER O_C A B H ;
  CIRCUMCENTER O A B C ;
SHOW: PARALLEL O_B C O_C B.

EXAMPLE  A111_5  
HYPOTHESES:
  POINT A B C ;
  ORTHOCENTER H A B C ;
  CIRCUMCENTER O_A B C H ;
  CIRCUMCENTER O_B A C H ;
  CIRCUMCENTER O_C A B H ;
  CIRCUMCENTER O A B C ;
SHOW: PERP_BISECT H O_B O_C.

