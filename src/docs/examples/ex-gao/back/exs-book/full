EXAMPLE  ORTH  
HYPOTHESES:
  POINT A B C ;
  FOOT E B A C ;
  FOOT F A B C ;
  INTERSECTION_LL H  A F  B E ;
SHOW:  PERPENDICULAR A B C H.



# Example 3.79 in
# << Machine Proof In Geometry>>
# World Scientific
#
EXAMPLE SIMONS
HYPOTHESES:
 CIRCLE A B C D;
 FOOT P D A B;
 FOOT Q D B C;
 FOOT R D A C;
SHOW: COLLINEAR P Q R.

EXAMPLE  A_293  
HYPOTHESES:
  CIRCLE A B C P P_1 ;
  CIRCUMCENTER O A B C ;
  MIDPOINT M P_1 P ;
  FOOT G P A B ;
  INTERSECTION_LP G_1  A B  P_1 P G ;
  FOOT F P A C ;
  INTERSECTION_LP F_1  A C  P_1 P F ;
  INTERSECTION_LL K  F_1 G_1  F G ;
SHOW:  EQANGLE F_1 K F P A P_1.

EXAMPLE  A_298  
HYPOTHESES:
  CIRCLE A B C D P P_1;
  CIRCUMCENTER O A B C;
  FOOT G P A B;
  FOOT F P A C;
  FOOT F_1 P A D;
  FOOT G_1 P_1 A B;
  FOOT F_2 P_1 A C;
  FOOT F_3 P_1 A D;
#  INTERSECTION_LP G_1  A B  P_1 P G ;
#  INTERSECTION_LP F_2  A C  P_1 P F ;
#  INTERSECTION_LP F_3  A D  P_1 P F_1 ;
SHOW:  EQANGLE F G F_1 F_2 G_1 F_3.

EXAMPLE  Miquel_point
HYPOTHESES:
  POINT A B C D;
  INTERSECTION_LL E A D B C;
  INTERSECTION_LL P A B C D;
  INTERSECTION_SS Q A B E C D E;
SHOW: COCIRCLE D A P Q.

# Example 6.134 in
# << Machine Proof In Geometry>>
# S.C. Chou, X.S. Gao, J.Z. Zhang
# World Scientific
#
EXAMPLE NPOINT
HYPOTHESES: POINT A B C;
  MIDPOINT L A B;
  MIDPOINT M B C;
  MIDPOINT N C A;
  FOOT D A B C;
SHOW: COCIRCLE L D M N.



EXAMPLE  D204  
HYPOTHESES:
  POINT A B C ;
  INTERSECTION_PP D A B C C A B;
  FOOT F A B D ;
  FOOT E B A C ;
  FOOT H C B D ;
  FOOT G D A C ;
SHOW: PARALLEL E F H G.

EXAMPLE  A_372
HYPOTHESES:
 POINT C E F;
 CIRCUMCENTER Q C E F;
 INTERSECTION_LC D C Q Q C;
 MIDPOINT I E F;
 INTERSECTION_LT P Q I E E Q;
 INTERSECTION_LC A C E P E;
SHOW: COLLINEAR F A D.

EXAMPLE  3_112
HYPOTHESES:
  POINT A B C;
  FOOT F C A B;
  FOOT D A B C;
  FOOT E B A C;
  FOOT G F B C;
  FOOT I D A B;
  FOOT H F A C;
  FOOT K E A B;
SHOW: COCIRCLE H I G K.

EXAMPLE  o65_3
HYPOTHESES:
 POINT O A B;
 FOOT C A O B;
 FOOT D B O A;
 ON_LINE M A B;
 FOOT P M A O;
 FOOT Q M O B;
 FOOT T Q O A;
 FOOT K P O B;
 INTERSECTION_LL S Q T P K;
SHOW: PERPENDICULAR O S P Q.

