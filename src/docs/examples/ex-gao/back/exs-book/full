
###theorems using full-angles
EXAMPLE  M_3
HYPOTHESES:
  POINT A B C;
  FOOT D C A B;
  FOOT E B A C;
  MIDPOINT F B C;
  MIDPOINT G D E;
SHOW: PERPENDICULAR G F D E.

EXAMPLE  A96_1  
HYPOTHESES:
  POINT A B C ;
  CIRCUMCENTER O A B C ;
  MIDPOINT A_1 B C ;
  MIDPOINT B_1 A C ;
  MIDPOINT C_1 A B ;
SHOW:  PERPENDICULAR O A_1 B_1 C_1.

EXAMPLE  A118_53  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT A_1 B C ;
  FOOT D A B C ;
  FOOT E B A C ;
  FOOT F C A B ;
  INTERSECTION_LL H  B E  C F ;
  MIDPOINT P B E ;
  MIDPOINT Q C F ;
SHOW:  COCIRCLE P Q H D.

EXAMPLE  A134_5  
HYPOTHESES:
  CIRCLE A B C D ;
  CIRCUMCENTER O A B C ;
  INTERSECTION_LL I  A D  B C ;
  MIDPOINT Q B C ;
  MIDPOINT S A D ;
  MIDPOINT J S Q ;
  SYM M J O;
# LRATIO M J O -1;
SHOW:  PERPENDICULAR M S B C.

EXAMPLE  A_224  
HYPOTHESES:
  POINT A B C ;
  ORTHOCENTER H A B C ;
  CIRCUMCENTER O A B C ;
  CIRCUMCENTER A_1 B C H ;
  CIRCUMCENTER B_1 A C H ;
  CIRCUMCENTER C_1 A B H ;
SHOW:  PARALLEL B_1 C_1 B C.

# Example 3.36 in
# << Machine Proof In Geometry>>
# S.C. Chou, X.S. Gao, J.Z. Zhang
# World Scientific
