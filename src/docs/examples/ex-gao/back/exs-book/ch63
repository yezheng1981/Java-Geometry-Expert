EXAMPLE  A65_4  
HYPOTHESES:
  CIRCLE A B C D ;
  CIRCUMCENTER O A B C ;
  FOOT E D B C ;
  FOOT F D A C ;
  FOOT G D A B ;
  INTERSECTION_PT E_1  D O A  A O A ;
  INTERSECTION_PT F_1  D O B  B O B ;
  INTERSECTION_PT G_1  D O C  C O C ;
SHOW: (SQ_DIS D E )(SQ_DIS D F )(SQ_DIS D G ) = (SQ_DIS D E_1 )(SQ_DIS D F_1 )(SQ_DIS D G_1 ).

EXAMPLE  A_110  
HYPOTHESES:
  POINT A B C ;
  CIRCUMCENTER O A B C ;
  MIDPOINT E A C ;
  MIDPOINT F A B ;
  MIDPOINT D B C ;
  INTERSECTION_LL G  A D  C F ;
SHOW:  +  3 (SQ_DIS O G ) + (SQ_DIS A G ) + (SQ_DIS G C ) + (SQ_DIS B G ) = 3 (SQ_DIS O A ).

#undo
EXAMPLE  A_79  
HYPOTHESES:
  POINT B E P ;
  ON_TLINE L E E B;
  LRATIO C E B -1;
  INTERSECTION_LB O  L E  B L ;
  INTERSECTION_LC A  B P  O B  ;
SHOW:  EQANGLE B A L L A C.

