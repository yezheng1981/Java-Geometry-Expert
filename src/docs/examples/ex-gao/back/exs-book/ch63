EXAMPLE  A111_9  
HYPOTHESES:
  POINT A B C ;
  ORTHOCENTER H A B C ;
  MIDPOINT M_1 B C ;
  MIDPOINT M_2 C A ;
  MIDPOINT M_3 A B ;
  MIDPOINT N1 M_1 M_2 ;
  MIDPOINT N2 M_1 M_3 ;
  INTERSECTION_TT N  N1 N1 M_1  N2 N2 M_1 ;
  ON_LINE X B C;
  FOOT H_1 H X N ;
  INTERSECTION_LP A_1  X N  A H H_1 ;
  INTERSECTION_LP B_1  X N  B H H_1 ;
  INTERSECTION_LP C_1  X N  C H H_1 ;
SHOW:  + (RATIO A A_1 H H_1 ) + (RATIO B B_1 H H_1 ) =  + (RATIO C_1 C H H_1 ) -  1 .

EXAMPLE  A118_49  
HYPOTHESES:
  POINT A B C ;
