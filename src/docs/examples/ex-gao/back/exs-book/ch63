  LRATIO B_2 A_1 C_1 Y_T;
  LRATIO C_2 B_1 A_1 Y_T;
  INTERSECTION_LL O  A A_2  B B_2 ;
  INTERSECTION_LL D  C C_2  A B ;
SHOW: (RATIO A D B D ) =  -1 .

