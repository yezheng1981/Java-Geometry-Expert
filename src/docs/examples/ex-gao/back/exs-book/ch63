EXAMPLE  A_572  
HYPOTHESES:
  POINT A B J ;
  INCENTER C A B J;
  FOOT X J A C ;
  FOOT Y J B C ;
  FOOT Z J A B ;
  MIDPOINT M_1 B C ;
  MIDPOINT M_2 A C ;
  MIDPOINT M_3 A B ;
  INTERSECTION_LL C1  X Y  C M_3 ;
  MIDPOINT C2 X Y ;
  LRATIO C3 C2 C1 -1;
  INTERSECTION_LL B1  Y Z  B M_2 ;
  MIDPOINT B2 Y Z ;
  LRATIO B3 B2 B1 -1;
  INTERSECTION_LL I  B B3  C C3 ;
  INTERSECTION_LL T1  X Z  A I ;
  INTERSECTION_LL T2  X Z  A M_1 ;
  MIDPOINT M T1 T2 ;
SHOW:  MIDPOINT M X Z.

EXAMPLE  A_635  
