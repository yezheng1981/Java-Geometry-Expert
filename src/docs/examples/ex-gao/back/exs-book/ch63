#6.3.1
EXAMPLE  A_87  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT M A B ;
  MIDPOINT N A C ;
SHOW:  PARALLEL M N B C.

EXAMPLE  A_88  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT F A C ;
  MIDPOINT E B C ;
  INTERSECTION_LL O  A E  B F ;
SHOW: (RATIO A O O E ) =  2 .

EXAMPLE  A_91  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT M A B ;
  MIDPOINT N A C ;
  MIDPOINT K B C ;
  INTERSECTION_PP P  A C M  K B N ;
  MIDPOINT L A K ;
SHOW: 4 (RATIO L P B C ) =  3.

EXAMPLE  A_93  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT M A B ;
  MIDPOINT N A C ;
  MIDPOINT K B C ;
  INTERSECTION_PP P  A C M  K B N ;
SHOW: 4 (AREA A K P ) = 3 (AREA A B C ).

EXAMPLE  A67_1  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT F A B ;
  MIDPOINT N C F ;
  INTERSECTION_LL K  B C  A N ;
SHOW: (RATIO B K K C ) =  2 .

EXAMPLE  A67_4  
HYPOTHESES:
  POINT A B C ;
  CENTROID G A B C ;
  INTERSECTION_LP P  A B  G B C ;
  INTERSECTION_LP Q  A C  G B C ;
SHOW: 4 (AREA P Q C B) = 5 (AREA A Q P ).

EXAMPLE  A69_5  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT D B C ;
  CENTROID G A B C ;
  HARMONIC L G D A ;
SHOW:  MIDPOINT D L A.

EXAMPLE  A68_8  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT F A B ;
  ON_LINE N C F;
  FOOT K N A C ;
  FOOT J N B C ;
SHOW: EQ_PRODUCT N K A C N J B C.

EXAMPLE  A158_2  
HYPOTHESES:
  POINT A B C ;
  CENTROID G A B C ;
  ON_LINE M A B;
  INTERSECTION_LL N  G M  A C ;
SHOW: (RATIO M B A M ) + (RATIO N C A N ) =  1 .

EXAMPLE  A158_8  
HYPOTHESES:
  POINT F E ;
  ON_BLINE A F E;
  ON_LINE C A F;
  ON_LINE B A E;
  MIDPOINT A_1 B C ;
  INTERSECTION_LL D  E F  A A_1 ;
SHOW: (RATIO E D F D )(RATIO E D F D) (PY A B B A) = (PY A C C A).

EXAMPLE  A68_10  
HYPOTHESES:
  POINT A B C X Y ;
  MIDPOINT D B C ;
  MIDPOINT E A C ;
  MIDPOINT F A B ;
  FOOT A1 A X Y ;
  FOOT B1 B X Y ;
  FOOT C1 C X Y ;
  FOOT D1 D X Y ;
  FOOT E1 E X Y ;
  FOOT F1 F X Y ;
SHOW:  +  1  + (RATIO B B1 A A1 ) + (RATIO C C1 A A1 ) 
       =  + (RATIO D D1 A A1 ) + (RATIO E E1 A A1 ) + (RATIO F F1 A A1).


EXAMPLE  A_106  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT M B C ;
SHOW: (SQ_DIS M A ) =  1 .

EXAMPLE  A70_11  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT N A B ;
  MIDPOINT M A C ;
  INTERSECTION_LL G  C N  B M ;
  LRATIO K B C ((1 R1));
  LRATIO K_1 C B ((1 R1));
  INTERSECTION_LL K_2  M N  A K ;
  INTERSECTION_LL Z  B C  K_2 G ;
SHOW: EQ_RATIO B Z Z C B K_1 K_1 C.

EXAMPLE  A_107  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT E A C ;
  MIDPOINT F A B ;
  MIDPOINT D B C ;
SHOW:   4 (SQ_DIS A D ) +  4 (SQ_DIS F C ) +  4 (SQ_DIS B E ) =
    +  3 (SQ_DIS A C ) +  3 (SQ_DIS A B ) +  3 (SQ_DIS B C ).

EXAMPLE  A71_1  
HYPOTHESES:
  POINT A B N M ;
  ON_BLINE G N M;
  MIDPOINT F A B ;
  LRATIO C F G 3;
SHOW:  + (SQ_DIS A N ) + (SQ_DIS N B ) + (SQ_DIS N C ) =  + (SQ_DIS A M ) + (SQ_DIS M B ) + (SQ_DIS M C ).

EXAMPLE  A158_1  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT A_1 B C ;
  MIDPOINT C_1 A B ;
  INTERSECTION_LP P  A A_1  C_1 B C ;
  INTERSECTION_LL Q  A B  P C ;
SHOW: 3 (RATIO Q A A B ) =  -1.

EXAMPLE  A_108  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT E A C ;
  LRATIO G B E (2) (3);
SHOW: 3 (SQ_DIS A G ) +  3 (SQ_DIS G C ) +  3 (SQ_DIS B G ) 
    =  (SQ_DIS A C ) + (SQ_DIS A B ) + (SQ_DIS B C ).

EXAMPLE  A_109  
HYPOTHESES:
  POINT A B C M ;
  MIDPOINT E A C ;
  LRATIO G B E (2) (3);
SHOW: 3 (SQ_DIS M G ) + (SQ_DIS A G ) + (SQ_DIS G C ) + (SQ_DIS B G )
      =  (SQ_DIS A M ) + (SQ_DIS M B ) + (SQ_DIS M C ).

EXAMPLE  A_324  
HYPOTHESES:
  POINT A B C ;
  LRATIO D B C r_1;
  LRATIO E C A r_2;
  LRATIO F A B r_3;
  LRATIO D_1 C B r_1;
  LRATIO E_1 A C r_2;
  LRATIO F_1 B A r_3;
SHOW: (AREA D_1 E_1 F_1 ) = (AREA D E F ).

EXAMPLE  A116_14  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT A_1 C B ;
  MIDPOINT B_1 A C ;
  MIDPOINT C_1 B A ;
  INTERSECTION_PP C_2  B C C_1  A B B_1 ;
  INTERSECTION_PP B_2  A B B_1  C A A_1 ;
  INTERSECTION_PP A_2  C A A_1  B C C_1 ;
SHOW: (AREA A_2 B_2 C_2 ) = 3 (AREA A B C ).

EXAMPLE  A70_12  
HYPOTHESES:
  POINT A B C ;
  MIDPOINT B_1 A C ;
  INTERSECTION_LP A_1  B C  B_1 A B ;
  MIDPOINT C_1 A B ;
  ON_LINE M A B;
  ON_LINE L A C;
  LRATIO L_1 B_1 L -1;
  LRATIO M_1 C_1 M -1;
  INTERSECTION_LL L_2  A_1 C_1  B L ;
  INTERSECTION_LL M_2  A_1 B_1  C M ;
  INTERSECTION_LL O  L_1 L_2  A A_1 ;
SHOW:  COLLINEAR O M_1 M_2.

EXAMPLE  A121_86  
HYPOTHESES:
  POINT A B C ;
  ON_LINE H B C;
  MIDPOINT A_1 B C ;
  INTERSECTION_LP N  C A  H A A_1 ;
  INTERSECTION_LL D  H N  A B ;
  MIDPOINT K N C ;
  MIDPOINT L B D ;
  LRATIO H_1 K H -1;
  LRATIO H_2 L H -1;
SHOW:  MIDPOINT A H_1 H_2.

EXAMPLE  A164_5  
HYPOTHESES:
  POINT A B C M ;
  MIDPOINT D B C ;
  MIDPOINT E C A ;
  MIDPOINT F A B ;
  INTERSECTION_LP P  A D  M B C ;
  INTERSECTION_LP Q  B E  M A C ;
  INTERSECTION_LP R  C F  M A B ;
  INTERSECTION_LL G  A D  C F ;
#  COLLINEAR G B E ;
SHOW:  + (RATIO G Q G B ) + (RATIO G R G C ) = (RATIO G P A G ).


